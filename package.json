    "babel-polyfill": "^6.26.0",
    "classlist-polyfill": "^1.0.3",
    "classnames": "^2.2.5",
    "custom-event-polyfill": "^0.3.0",
    "es6-promise": "^4.1.0",
    "html-to-element": "^1.0.0",
    "lodash.throttle": "^4.1.1",
    "stylenames": "^1.1.5",
    "uuid": "^3.0.1",
