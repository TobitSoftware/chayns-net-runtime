{
  "env": {
    "browser": true
  },
  "globals": {
    "chayns": true
  },
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": "off",
    "indent": "off",
    "strict": 0,
    "comma-dangle": "off",
    "max-len": "off",
    "no-use-before-define": "off",
    "import/no-extraneous-dependencies": "off",
    "lines-between-class-members": [
      "error",
      "always",
      {
        "exceptAfterSingleLine": true
      }
    ]
  },
  "extends": "airbnb-base"
}